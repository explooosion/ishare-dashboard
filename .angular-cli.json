{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "dasboard"
  },
  "apps": [{
    "root": "src",
    "outDir": "dist",
    "assets": [
      "assets",
      "favicon.ico"
    ],
    "index": "index.html",
    "main": "main.ts",
    "polyfills": "polyfills.ts",
    "test": "test.ts",
    "tsconfig": "tsconfig.app.json",
    "testTsconfig": "tsconfig.spec.json",
    "prefix": "app",
    "styles": [
      "styles.css",
      "./plugin/font-awesome/css/font-awesome.min.css",
      "./plugin/bootstrap/css/bootstrap.min.css",
      "./plugin/datatables/dataTables.bootstrap4.css",
      "./assets/css/sb-admin.min.css"
    ],
    "scripts": [
      "./plugin/tether/tether.min.js",
      "./plugin/jquery/jquery.min.js",
      "./plugin/jquery-easing/jquery.easing.min.js",
      "./plugin/bootstrap/js/bootstrap.min.js",
      "./plugin/datatables/jquery.dataTables.js",
      "./plugin/datatables/dataTables.bootstrap4.js",
      "./plugin/chart.js/Chart.bundle.min.js"
    ],
    "environmentSource": "environments/environment.ts",
    "environments": {
      "dev": "environments/environment.ts",
      "prod": "environments/environment.prod.ts"
    }
  }],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [{
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
